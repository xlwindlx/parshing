<p id="notice"><%= notice %></p>

<h1>16년도 2학기 전공</h1>

<table>
  <thead>
    <tr>
      <th>Div</th>
      <th>Title</th>
      <th>Grade</th>
      <th>Grades</th>
      <th>Subject</th>
      <th>Proffesion</th>
      <th>Day</th>
      <th>Time</th>
      <th>Classroom</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profs.each do |prof| %>
      <tr>
        <td><%= prof.div %></td>
        <td><%= prof.title %></td>
        <td><%= prof.grade %></td>
        <td><%= prof.grades %></td>
        <td><%= prof.subject %></td>
        <td><%= prof.proffesion %></td>
        <td><%= prof.day %></td>
        <td><%= prof.time %></td>
        <td><%= prof.classroom %></td>
        <td><%= link_to '자세히 보기', prof %></td>
        <td><%= link_to '수정', edit_prof_path(prof) %></td>
        <td><%= link_to '삭제', prof, method: :delete, data: { confirm: '삭제할까요?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '추가하기', new_prof_path %>
